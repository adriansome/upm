{
    "attributes": {
        
        "arrival_date": 
        {
            
            "type" : "date",
            "format": "d/m/Y",
            "label": "Arrival date",
            "validation": ">today"
        },
                
        "departure_date":
        {
            
            "type": "date",
            "format": "d/m/Y",
            "label": "Departure Date",
            "validation": ">arrival_date"
        },
        
        "type": 
        {
            "type": "list",
            "values":
            {
                "1": "Single",
                "2": "Double",
                "3": "Twin",
                "4": "Whole house"
            }        
        },
                
        "status":
        {
            "type": "hidden",
            "default": "available" 
        },
                
        "number_of_bedrooms":
        {
            "type": "singleline",
            "label": "Number of bedrooms"
        },
                
        "sleeps_number":
        {
            "type": "singleline",
            "label": "Number of people it sleeps"
        },
        
        "property_id":
        {
            "type": "hidden"
        },
                
        "booked_by":
        {
            "type": "hidden"
        },
                
        "title":
        {
            "type": "hidden"
        }
        
    }
}
